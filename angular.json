{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "reactive-flexible": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        },
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "projects/reactive-flexible",
      "sourceRoot": "projects/reactive-flexible/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/reactive-flexible",
            "index": "projects/reactive-flexible/src/index.html",
            "main": "projects/reactive-flexible/src/main.ts",
            "polyfills": "projects/reactive-flexible/src/polyfills.ts",
            "tsConfig": "projects/reactive-flexible/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/reactive-flexible/src/favicon.ico",
              "projects/reactive-flexible/src/assets"
            ],
            "styles": [
              "projects/reactive-flexible/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "projects/reactive-flexible/src/environments/environment.ts",
                  "with": "projects/reactive-flexible/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "reactive-flexible:build:production"
            },
            "development": {
              "browserTarget": "reactive-flexible:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "reactive-flexible:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/reactive-flexible/src/test.ts",
            "polyfills": "projects/reactive-flexible/src/polyfills.ts",
            "tsConfig": "projects/reactive-flexible/tsconfig.spec.json",
            "karmaConfig": "projects/reactive-flexible/karma.conf.js",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/reactive-flexible/src/favicon.ico",
              "projects/reactive-flexible/src/assets"
            ],
            "styles": [
              "projects/reactive-flexible/src/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/reactive-flexible/**/*.ts",
              "projects/reactive-flexible/**/*.html"
            ]
          }
        }
      }
    },
    "@gpeel/my-validators": {
      "projectType": "library",
      "root": "projects/gpeel/my-validators",
      "sourceRoot": "projects/gpeel/my-validators/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/gpeel/my-validators/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/gpeel/my-validators/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/gpeel/my-validators/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/gpeel/my-validators/src/test.ts",
            "tsConfig": "projects/gpeel/my-validators/tsconfig.spec.json",
            "karmaConfig": "projects/gpeel/my-validators/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/gpeel/my-validators/**/*.ts",
              "projects/gpeel/my-validators/**/*.html"
            ]
          }
        }
      }
    },
